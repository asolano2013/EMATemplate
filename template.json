            "apiVersion": "2019-11-01",
            "properties": {
                "description": "nsgRuleDescription",
                "protocol": "*",
                "sourcePortRange": "*",
                "destinationPortRange": "8080",
                "sourceAddressPrefix": "nsgRuleSourceAddressPrefix",
                "destinationAddressPrefix": "nsgRileDestinationAddressRange",
                "access": "Allow",
                "priority": 340,
                "direction": "Inbound"
            }
        },
        {
            "name": "Port8172",
            "type": "Microsoft.Network/networkSecurityGroups/securityRules",
            "apiVersion": "2019-11-01",
            "properties": {
                "description": "nsgRuleDescription",
                "protocol": "*",
                "sourcePortRange": "*",
                "destinationPortRange": "8172",
                "sourceAddressPrefix": "nsgRuleSourceAddressPrefix",
                "destinationAddressPrefix": "nsgRileDestinationAddressRange",
                "access": "Allow",
                "priority": 1010,
                "direction": "Inbound"
            }
        },
        {
            "name": "Port443OB",
            "type": "Microsoft.Network/networkSecurityGroups/securityRules",
            "apiVersion": "2019-11-01",
            "properties": {
                "description": "Port 443 Outbound",
                "protocol": "*",
                "sourcePortRange": "*",
                "destinationPortRange": "443",
                "sourceAddressPrefix": "nsgRuleSourceAddressPrefix",
                "destinationAddressPrefix": "nsgRileDestinationAddressRange",
                "access": "Allow",
                "priority": 100,
                "direction": "Outbound"
            }
        },
        {
            "name": "Port8000",
            "type": "Microsoft.Network/networkSecurityGroups/securityRules",
            "apiVersion": "2019-11-01",
            "properties": {
                "description": "nsgRuleDescription",
                "protocol": "*",
                "sourcePortRange": "*",
                "destinationPortRange": "8000",
                "sourceAddressPrefix": "nsgRuleSourceAddressPrefix",
                "destinationAddressPrefix": "nsgRileDestinationAddressRange",
                "access": "Allow",
                "priority": 110,
                "direction": "Outbound"
            }
        },
